  "devDependencies": {
    "autoprefixer": "^10.4.21",
    "esbuild": "^0.25.5",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.11",
    "typescript": "^5.8.3",
    "vite": "^7.0.0"
  }
}
